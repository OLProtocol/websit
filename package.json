{
  "name": "miniapp",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --host --mode debug",
    "build:app": "node ./updateVersion.js && tsc && vite build --mode app",
    "build:app.testnet4": "node ./updateVersion.js && tsc && vite build --mode app.testnet4",
    "build:dev": "node ./updateVersion.js && tsc && vite build --mode dev",
    "build:dev.testnet4": "node ./updateVersion.js && tsc && vite build --mode dev.testnet4",
    "build:test": "node ./updateVersion.js && tsc && vite build  --mode test",
    "build:test.testnet4": "node ./updateVersion.js && tsc && vite build  --mode test.testnet4",
    "dbuild": "tsc && vite build --minify false",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
